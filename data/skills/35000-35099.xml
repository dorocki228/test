	<skill id="35017" levels="7" name="Талисман Дерзости I">
		<set name="abnormal_effect" value="D_TALI_DECO_E"/>
		<set name="abnormal_level" value="3"/>
		<set name="abnormal_time" value="-1"/>
		<set name="abnormal_type" value="seed_talisman"/>
		<set name="icon" value="icon.tallisman_baium"/>
		<set name="operate_type" value="P"/>
		<set name="skillType" value="BUFF"/>
		<set name="target" value="TARGET_SELF"/>
		<for>
			<mul order="0x30" stat="pAtk" value="#pAtk"/>
			<mul order="0x30" stat="mAtk" value="#mAtk"/>
			<add order="0x40" stat="magicResist" value="#mRes"/>
			<mul order="0x30" stat="pSkillPower" value="#pSkillPower"/>
			<mul order="0x30" stat="swordWpnVuln" value="#WpnVuln"/>
			<mul order="0x30" stat="bluntWpnVuln" value="#WpnVuln" />
			<mul order="0x30" stat="dualWpnVuln" value="#WpnVuln" />
			<mul order="0x30" stat="daggerWpnVuln" value="#WpnVuln" />
			<mul order="0x30" stat="bowWpnVuln" value="#WpnVuln" />
			<mul order="0x30" stat="poleWpnVuln" value="#WpnVuln" />
			<mul order="0x30" stat="fistWpnVuln" value="#WpnVuln" />
			<add order="0x10" stat="DEX" value="#baseStatsDEX" />
			<add order="0x10" stat="WIT" value="#baseStatsWIT" />
			<add order="0x10" stat="CON" value="#baseStatsCON" />
			<add order="0x10" stat="MEN" value="#baseStatsMEN"/>
			<add order="0x10" stat="INT" value="#baseStatsINT"/>
			<add order="0x10" stat="STR" value="#baseStatsSTR"/>
			<add order="0x40" stat="baseMCritRate" value="#crate"/>
			<add order="0x40" stat="basePCritRate" value="#crate"/>
			<mul order="0x30" stat="mpConsum" value="#mpConsum"/>
			<mul order="0x30" stat="mpConsumePhysical" value="#mpConsum"/>
            <mul order="0x30" stat="mpDanceConsume" value="#mpConsum"/>
            <mul order="0x30" stat="mReuse" value="#reuse"/>
            <mul order="0x30" stat="pReuse" value="#reuse"/>
            <mul order="0x30" stat="musicReuse" value="#reuse"/>
        </for>
        <triggers>
            <trigger cancel_effects_on_remove="true" id="#triggerId" level="#triggerLevel" type="ADD"/>
        </triggers>
        <table name="#triggerLevel">0 0 0 1 2 3 4</table>
        <table name="#triggerId">0 0 0 35018 35018 35018 35018</table>
        <table name="#pAtk">1.01 1.02 1.03 1.04 1.05 1.06 1.08</table>
        <table name="#mAtk">1.01 1.02 1.03 1.04 1.05 1.06 1.08</table>
        <table name="#mRes">0 2 3 4 5 6 8</table>
        <table name="#pSkillPower">1 1 1.01 1.02 1.02 1.03 1.05</table>
        <table name="#WpnVuln">1 1 0.99 0.98 0.98 0.97 0.95</table>
        <table name="#baseStatsCON">0 0 0 1 1 1 1</table>
		<table name="#baseStatsMEN">0 0 0 1 1 1 1</table>
		<table name="#baseStatsDEX">0 0 0 0 1 1 1</table>
		<table name="#baseStatsWIT">0 0 0 0 1 1 1</table>
		<table name="#baseStatsINT">0 0 0 0 0 0 1</table>
		<table name="#baseStatsSTR">0 0 0 0 0 0 1</table>
        <table name="#mpConsum">1 1 1 1 0.98 0.96 0.95</table>
        <table name="#reuse">1 1 1 1 1 1 0.97</table>
        <table name="#crate">0 0 0 0 0 10 15</table>
    </skill>
