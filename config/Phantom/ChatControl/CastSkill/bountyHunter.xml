<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE list SYSTEM "ChatControl.dtd">
	<!-- Использование умений по команде в чат
	Поддерживаемые типы:
	castSelfBuff	 		- селф бафы
	castTargetSkill			- умения по таргенту хозяина
	castAttackingSkill		- атакующие умения
	castBuff				- бафы по таргенту
	castPartyIcon			- разного рода иконы\щилки (умения с зарядом)
	castLimitBuffSkill		- лимиты 
	-->
<list>
	<ChatControl class_id="55">
		<skill id="34" type="castSelfBuff" text_commands="перевязка,перевяз,bandage"/>
	</ChatControl>
 </list>