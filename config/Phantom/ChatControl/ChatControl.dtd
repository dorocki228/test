<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT skill EMPTY>
<!ATTLIST skill
	id CDATA #REQUIRED
	text_commands CDATA #REQUIRED
	type CDATA #REQUIRED
>
<!ELEMENT list ((ChatControl+))>
<!ELEMENT command EMPTY>
<!ATTLIST command
	ChatCommands CDATA #REQUIRED
	ServantMode (SUPPORT | PROTECTION | STOP | FARM | ASSIST) #REQUIRED
	text_commands CDATA #REQUIRED
>
<!ELEMENT ChatOK EMPTY>
<!ATTLIST ChatOK
	chance CDATA #REQUIRED
	text CDATA #REQUIRED
>
<!ELEMENT ChatDead EMPTY>
<!ATTLIST ChatDead
	chance CDATA #REQUIRED
	text CDATA #REQUIRED
>
<!ELEMENT ChatControl ((skill+ | (command+, ChatOK, ChatDead)))>
<!ATTLIST ChatControl
	class_id CDATA #REQUIRED
>
